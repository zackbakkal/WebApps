                "content": "<body>"
            },
            {
                "class": "indentTwice",
                "content": "<form>"
            },
            {
                "class": "indentTrice",
                "content": "Phone number:"
            },
            {
                "class": "indentTrice",
                "content": "<input type=\"tel\" name=\"yourtel\" pattern=\"\\d{3}-\\d{3}-\\d{4}>"
            },
            {
                "class": "indentTwice",
                "content": "</form>"
            },
            {
                "class": "indentOnce",
                "content": "</body>"
            },
            {
                "class": "",
                "content": "<html>"
            }
        ]
    },

    "date": {
        "id": "dateType",
        "title": "date Input Type",
        "description": [
            "The &lt;input type=\"date\"&gt; is used for input fields that should contain date.",
            "Depending on the browser support, a date picker can show up in the input field.",
            "You can also add the min and max attributes to add restrictions to dates."
        ],
        "example": [
            {
                "class": "",
                "content": "<!DOCTYPE html>"
            },
            {
                "class": "",
                "content": "<html>"
            },
            {
                "class": "indentOnce",
                "content": "<head>"
            },
            {
                "class": "indentTwice",
                "content": "<title>date input type</title>"
            },
            {
                "class": "indentOnce",
                "content": "</head>"
            },
            {
                "class": "indentOnce",
                "content": "<body>"
            },
            {
                "class": "indentTwice",
                "content": "<form>"
            },
            {
                "class": "indentTrice",
                "content": "Birthday:"
            },
            {
                "class": "indentTrice",
                "content": "<input type=\"date\" name=\"birthday\" min=\"2000-01-01\" max=\"2019-03-15\">"
            },
            {
                "class": "indentTwice",
                "content": "</form>"
            },
            {
                "class": "indentOnce",
                "content": "</body>"
            },
            {
                "class": "",
                "content": "<html>"
            }
        ]
    },
